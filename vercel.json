{
  "rewrites": [{ "source": "/(.*)", "destination": "/" }],
  "routes": [
    {
      "src": "/",
      "dest": "/home"
    },
    {
      "src": "/home",
      "dest": "/home"
    }
  ],
  "version": 2,
  "builds": [
    {
      "src": "vite.config.js",
      "use": "@vercel/vite"
    }
  ],
  "public": ["public"]
}
